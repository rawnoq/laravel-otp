{
    "name": "rawnoq/laravel-otp",
    "description": "A professional OTP (One-Time Password) package for Laravel",
    "keywords": ["laravel", "otp", "one-time-password", "verification", "authentication"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rawnoq",
            "email": "info@rawnoq.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Rawnoq\\LaravelOtp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rawnoq\\LaravelOtp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rawnoq\\LaravelOtp\\OtpServiceProvider"
            ],
            "aliases": {
                "Otp": "Rawnoq\\LaravelOtp\\Facades\\Otp"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

